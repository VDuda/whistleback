# Story Protocol TestNet Configuration
# Copy to .env.testnet and fill in your values

# Story TestNet RPC
STORY_RPC_URL=https://odyssey.storyrpc.io/
STORY_CHAIN_ID=1315

# Wallet Configuration
STORY_PRIVATE_KEY=your_testnet_private_key_here
STORY_DEPLOYER_ADDRESS=0x_your_address_here

# Story Protocol Contract Addresses (Aeneid TestNet)
STORY_IP_ASSET_REGISTRY=0x77319B4031e6eF1250907aa00018B8B1c67a244b
STORY_LICENSING_MODULE=0x04fbd8a2e56dd85CFD5500A4A4DfA955B9f1dE6f
STORY_ROYALTY_MODULE=0xD2f60c40fEbccf6311f8B47c4f2Ec6b040400086
STORY_PILICENSE_TEMPLATE=0x2E896b0b2Fdb7457499B56AAaA4AE55BCB4Cd316
STORY_METADATA_MODULE=0x6E81a25C99C6e8430aeC7353325EB138aFE5DC16
STORY_GROUPING_MODULE=0x69D3a7aa9edb72Bc226E745A7cCdd50D947b69Ac
STORY_DISPUTE_MODULE=0x9b7A9c70AFF961C799110954fc06F3093aeb94C5

# Constellation Network TestNet Configuration
# Backend-focused: WhistleBack backend has Stargazer wallet
# Users don't need Stargazer (handled invisibly)

# Constellation Network Mode (mock or real)
CONSTELLATION_MODE=mock

# Constellation L0/L1 URLs
CONSTELLATION_L0_URL=https://l0-lb-testnet.constellationnetwork.io
CONSTELLATION_L1_URL=https://l0-lb-testnet.constellationnetwork.io

# Stargazer Wallet (Backend)
# Generate using: https://play.google.com/store/apps/details?id=com.stargazer
CONSTELLATION_PRIVATE_KEY=your_stargazer_private_key_here

# Metagraph ID (if using custom metagraph)
CONSTELLATION_METAGRAPH_ID=your_metagraph_id_here

# Next.js Configuration
NEXT_PUBLIC_STORY_RPC_URL=https://odyssey.storyrpc.io/
NEXT_PUBLIC_STORY_CHAIN_ID=1315
NEXT_PUBLIC_CONSTELLATION_L0_URL=https://l0-lb-testnet.constellationnetwork.io
